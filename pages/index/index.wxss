/* pages/index/index.wxss */
.ad01{
  width: 100%;
}


.qukuai{display: flex;align-items:center;background-color: #f2f2f2;}
.qukuai_zuo{margin: 5px 10px; width:5px; height:20px; background-color: rgb(13, 163, 233);border-radius: 10px;}
.qukuai_zhong{font-size: 16px; flex-grow: 1;}
.qukuai_you{font-size: 14px; color: gray;padding-right: 10px;}


.xinxi {
  display: flex;
  align-items: center;
  padding: 5px;
  border-bottom: 1px solid #f2f2f2;
  font-size: 15px;
}

.xinxi-icon {
  width: 22px;
  height: 22px;
}

.xinxi-text {
  flex-grow: 1;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.xinxi-arrow {
  width: 13px;
  height: 13px;
}



.shuoming{
  padding: 5px;
  color: gray;
}
.shuoming_biaoti{
  font-weight: bold;
  color: orangered;
  border-bottom: 1px solid gainsboro;
  padding: 5px 0px;
}
.shuoming_xiangmu{
  padding: 5px 0px;
  border-bottom: 1px dotted  gainsboro;
  font-size: 14px;
}
.yuyue{
  background-color: orangered;
  color: white;
  text-align: center;
  margin: 10px;
  padding: 10px;
  border-radius: 5px;
}

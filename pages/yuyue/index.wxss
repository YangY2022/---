.jiaoshi{
  display: flex; 
  flex-wrap: wrap;
  border-bottom: 3px solid orangered;
}
.jiaoshi_list{
  width: 25%;
  padding: 10px;
  box-sizing: border-box;
  text-align: center;
  background-color: gainsboro;
  border-radius: 5px;
  border:3px solid white;
}
.jiaoshi_xuanzhong{
  background-color: orangered;
  color: white;
}

